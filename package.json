{
  "name": "@stoplight/elements-starter-angular",
  "version": "1.0.0",
  "description": "Example Angular project utilizing Stoplight Elements",
  "author": "Stoplight <support@stoplight.io>",
  "homepage": "https://github.com/stoplightio/elements-starter-angular#readme",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stoplightio/elements-starter-angular.git"
  },
  "scripts": {
    "start": "ng serve",
    "build": "ng build --prod --base-href '/elements-starter-angular'",
    "postinstall": "ngcc --tsconfig './tsconfig.app.json'",
    "lint": "ng lint",
    "serve-prod": "angular-http-server --path ./dist/angular -p 4200",
    "e2e:execute": "cypress run",
    "e2e:open": "cypress open",
    "e2e:run": "start-server-and-test serve-prod http://localhost:4200 e2e:execute"
  },
  "bugs": {
    "url": "https://github.com/stoplightio/elements-starter-angular/issues"
  },
  "dependencies": {
    "@angular/animations": "~11.0.3",
    "@angular/common": "~11.0.3",
    "@angular/compiler": "~11.0.3",
    "@angular/core": "~11.0.3",
    "@angular/forms": "~11.0.3",
    "@angular/platform-browser": "~11.0.3",
    "@angular/platform-browser-dynamic": "~11.0.3",
    "@angular/router": "~11.0.3",
    "@stoplight/elements-web-components": "^6.1.1",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/cli": "~11.0.3",
    "@angular/compiler-cli": "~11.0.3",
    "@angular/language-service": "~12.0.3",
    "@angular-devkit/build-angular": "~0.1100.3",
    "@angular-eslint/builder": "0.8.0-beta.3",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.3",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.3",
    "@angular-eslint/schematics": "^0.8.0-beta.3",
    "@angular-eslint/template-parser": "0.8.0-beta.3",
    "@stoplight/eslint-config": "^1.2.0",
    "@types/node": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.13.0",
    "angular-http-server": "~1.9.0",
    "cypress": "^6.2.0",
    "prettier": "^2.2.1",
    "start-server-and-test": "^1.11.5",
    "typescript": "~4.0.5",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "^3.2.0"
  }
